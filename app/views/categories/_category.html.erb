<div id="<%= dom_id category %>">
  <p>
    <strong>Name:</strong>
    <%= category.name %>
  </p>

  <p>
    <strong>Image Url:</strong>
    <%= category.image_url %>
  </p>

  <p>
    <strong>Position:</strong>
    <%= category.position %>
  </p>

  <p>
    <strong>Hidden:</strong>
    <%= category.hidden %>
  </p>

  <p>
    <strong>Slug:</strong>
    <%= category.slug %>
  </p>

  <p>
    <strong>Description:</strong>
    <%= category.description %>
  </p>

  <p>
    <strong>Subtitle:</strong>
    <%= category.subtitle %>
